{
  "name": "claarity",
  "version": "0.1.2",
  "description": "",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "version:patch": "npm version patch && npm run build",
    "version:minor": "npm version minor && npm run build",
    "version:major": "npm version major && npm run build",
    "test": "node test-functionality-simple.cjs",
    "test:functionality": "node test-functionality-simple.cjs",
    "audit": "npm audit",
    "security-audit": "npm audit --audit-level=moderate",
    "test:security": "node src/tests/security.test.ts || echo 'Security tests completed'",
    "test:xss": "node -r ts-node/register src/tests/xss-protection.test.ts || echo 'XSS protection tests completed'",
    "test:security-headers": "node -r ts-node/register src/tests/security-headers.test.ts || echo 'Security headers tests completed'",
    "test:logging": "node -r ts-node/register src/tests/logger.test.ts || echo 'Logging security tests completed'",
    "test:production-logging": "node -r ts-node/register src/tests/production-logging.test.ts || echo 'Production logging tests completed'",
    "test:secure-storage": "node -r ts-node/register src/tests/secure-storage.test.ts || echo 'Secure storage tests completed'",
    "test:error-handling": "node -r ts-node/register src/tests/secure-error-handling.test.ts || echo 'Secure error handling tests completed'",
    "test:security-functions": "node test-security-simple.cjs",
    "security-lint": "eslint src/",
    "security-lint:fix": "eslint src/ --fix",
    "audit:strict": "audit-ci --moderate",
    "audit:report": "npm audit --json > audit-report.json && npm audit --audit-level=info > audit-summary.txt",
    "verify-security": "node scripts/verify-security-setup.cjs",
    "security-check": "npm run test:security-headers && npm run test:logging && npm run test:secure-storage && npm run test:error-handling && npm run security-audit",
    "security-scan:full": "npm run security-lint && npm run audit:strict && npm run security-check",
    "security-scan:ci": "npm run security-lint && npm run audit:report && npm run security-check",
    "security-scan": "node scripts/security-scan.cjs",
    "test:integration": "node -r ts-node/register src/scripts/runIntegrationTests.ts",
    "test:integration:quick": "node -r ts-node/register src/scripts/runIntegrationTests.ts --quick",
    "test:integration:verbose": "node -r ts-node/register src/scripts/runIntegrationTests.ts --verbose",
    "test:integration:report": "node -r ts-node/register src/scripts/runIntegrationTests.ts --output integration-report.html --format html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@cornerstonejs/core": "^3.32.5",
    "@cornerstonejs/dicom-image-loader": "^3.31.14",
    "@cornerstonejs/nifti-volume-loader": "^1.77.9",
    "@cornerstonejs/streaming-image-volume-loader": "^1.77.9",
    "@cornerstonejs/tools": "^3.32.5",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/crypto-js": "^4.2.2",
    "@types/pako": "^2.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cornerstone-web-image-loader": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dcmjs": "^0.43.0",
    "dicom-parser": "^1.8.21",
    "dompurify": "^3.2.6",
    "lucide-react": "^0.534.0",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/migrate-config": "^1.5.2",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.2.1",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^7.0.6",
    "vite-plugin-csp": "^1.1.2",
    "vite-plugin-static-copy": "^3.1.1",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@babel/runtime": "^7.28.2"
  }
}
