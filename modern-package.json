  "dependencies": {
    "@cornerstonejs/core": "^1.77.9",
    "@cornerstonejs/tools": "^1.77.9",
    "@cornerstonejs/streaming-image-volume-loader": "^1.77.9",
    "@cornerstonejs/dicom-image-loader": "^1.77.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "dicom-parser": "^1.8.21",
    "cornerstone-wado-image-loader": "^4.13.3",
    "@types/uuid": "^9.0.7",
    "uuid": "^9.0.1",
    "lucide-react": "^0.395.0",
    "clsx": "^2.1.1",
